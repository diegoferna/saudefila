{
    "compilerOptions": {
     "target": "es2020",   
     "outDir": "./dist",
     "rootDir": "./src",
     "esModuleInterop": true,             
     "forceConsistentCasingInFileNames": true,           
     "strict": true,                                    
     "skipLibCheck": true,
     "allowJs": false,
     "module": "ES2015",
     "moduleResolution": "Bundler",
    },
    "include": [
      "src/**/*.ts",
  
    ],
    "exclude": [
      "db/migrations/*"
    ]
  }
  